  "description": "A very basic tip calculator using React and Material UI.",
  "license": "MIT",
  "homepage": "https://mikedpad.github.io/tip-calculator",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikedpad/tip-calculator.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint 'src/**/*.{js,jsx}'",
